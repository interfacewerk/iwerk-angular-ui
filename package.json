{
  "name": "iwerk-angular-ui",
  "version": "0.0.0-development",
  "license": "MIT",
  "angular-cli": {},
  "author": "interfacewerk GmbH",
  "main": "npm run ng s",
  "release": {
    "publish": [
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/interfacewerk/iwerk-angular-ui.git"
  },
  "scripts": {
    "commit": "git-cz",
    "ng": "ng",
    "serve-aot": "ng serve --aot=true",
    "build-aot": "ng build --aot=true",
    "build": "ng-packagr -p package.json",
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "test-once": "ng test --watch=false",
    "test-ci": "ng test --watch=false --code-coverage",
    "send-coverage": "cat coverage/lcov.info | coveralls",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e-ci": "ng e2e",
    "e2e": "./scripts/e2e.dev.sh",
    "build:dev": "./scripts/watch-build.dev.sh",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.3.3",
    "codelyzer": "^4.3.0",
    "commitizen": "^2.9.6",
    "core-js": "^2.4.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "2.0.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "ng-packagr": "^2.4.1",
    "nodemon": "^1.17.3",
    "protractor": "~5.1.2",
    "rxjs": "^5.5.0",
    "semantic-release": "^15.4.0",
    "travis-deploy-once": "^5.0.0",
    "ts-node": "~6.1.1",
    "tsickle": "^0.27.5",
    "tslib": "1.9.0",
    "tslint": "^5.0.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.4"
  },
  "peerDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.0",
    "zone.js": "^0.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    }
  }
}
