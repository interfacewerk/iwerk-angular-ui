{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "iwerk-angular-ui",
  "version": "0.0.0-development",
  "license": "MIT",
  "angular-cli": {},
  "author": "interfacewerk GmbH",
  "main": "npm run ng s",
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "../CHANGELOG.md"
      },
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "../CHANGELOG.md"
        ]
      }
    ],
    "publish": [
      "@semantic-release/npm"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/interfacewerk/iwerk-angular-ui.git"
  },
  "scripts": {
    "commit": "git-cz",
    "ng": "ng",
    "serve-aot": "ng serve --aot=true",
    "build-aot": "ng build --aot=true",
    "build": "ng-packagr -p package.json",
    "build:docs": "npm run compodoc && npm run ng -- build demo --output-path=docs --base-href https://interfacewerk.github.io/iwerk-angular-ui/",
    "start": "ng serve",
    "start:docs": "ng serve demo",
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "test-once": "ng test --watch=false",
    "test-ci": "ng test --watch=false --code-coverage",
    "send-coverage": "cat coverage/lcov.info | coveralls",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e-ci": "ng e2e",
    "e2e": "./scripts/e2e.dev.sh",
    "build:dev": "./scripts/watch-build.dev.sh",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "compodoc": "./node_modules/.bin/compodoc --output projects/demo/src/assets/documentation -p src/tsconfig.app.json --exportFormat json"
  },
  "dependencies": {
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.4",
    "@angular/cli": "^6.2.4",
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/compiler-cli": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "@compodoc/compodoc": "^1.1.6",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.3.3",
    "codelyzer": "^4.3.0",
    "commitizen": "^3.0.0",
    "core-js": "^2.4.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^1.0.2",
    "ng-packagr": "^2.4.1",
    "nodemon": "^1.17.3",
    "protractor": "~5.4.0",
    "rxjs": "^6.3.3",
    "semantic-release": "^15.8.0",
    "travis-deploy-once": "^5.0.0",
    "ts-node": "~7.0.1",
    "tsickle": "0.27.5",
    "tslib": "1.9.3",
    "tslint": "^5.0.0",
    "typescript": "2.9.2",
    "zone.js": "^0.8.26"
  },
  "peerDependencies": {
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "core-js": "^2.4.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "tslib",
      "@fortawesome/angular-fontawesome",
      "@fortawesome/fontawesome-svg-core",
      "@fortawesome/free-solid-svg-icons"
    ]
  }
}
